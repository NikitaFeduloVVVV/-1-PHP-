<?php
//Функция для вычисления длины строки
function getLength(string $inputStr): int 
{
    return strlen($inputStr);
}
//Функция для получения строки
function displayLen(int $length): string 
{
    return "Длина строки: $length\n";
}
$inputStr = "Lit Energy";
$length = getLength($inputStr);
$result = displayLen($length);
echo $result;
?>
